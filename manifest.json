{
	"name": "My plugin",
	"version": "0.0.1",
	"description": "Have one cart for items from multiple online stores",
	"manifest_version": 2,
	"browser_action": {
		"default_popup": "./index.html",
		"default_title": "Open the popup",
		"default_icon": "./defaultIcon.jpg"
	},
	"content_scripts": [
		{
			"js": ["content.js"],
			"css": ["content.css"],
			"matches": ["https://*.google.com/*"]
		}
	]
}
